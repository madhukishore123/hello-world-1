pipeline{
agent any
stages{
   stage("BUild"){
     steps{
       echo "app is building"
          }
          }
   stage("Test"){
     steps{
     def i = 10;
     def j = 20;
     z=j
      
        }
        }
   stage("Deploy"){
     steps{
       echo "app is Deploying"
       }
       }
}
post{
always{
echo "always execute"
}
failure{
echo "only failure"
}
}
}
