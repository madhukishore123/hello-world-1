pipeline{
agent any
stages{
   stage("BUild"){
     steps{
       echo "app is building"
          }
          }
   stage("Test"){
     steps{
       echo "app is Test"
        }
        }
   stage("Deploy"){
     steps{
       echo "app is Deploying"
       }
       }
}
post{
always{
echo "always execute"
}
faiure{
echo "only failure"
}
}
}
